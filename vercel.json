{
  "rewrites": [
    { "source": "/api/provinces", "destination": "/server.js" },
    {
      "source": "/api/regencies/*",
      "destination": "/api/regencies/*"
    },
    {
      "source": "/api/districts/*",
      "destination": "/api/districts/*"
    },
    {
      "source": "/api/villages/*",
      "destination": "/api/villages/*"
    },
    { "source": "/(.*)", "destination": "/server.js" }
  ]
}
